<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>
<%= link_to "Make a new article", "/articles/new"  %>

<% @article.each do |article|  %>
<div class="container">

<p>ID: <%= article.id %></p>
<p>TITLE:<%= link_to article.title, article_path(article)  %> </p>
<p>CONTENT:<%= article.content %></p>
<p><%= link_to "Delete article", article_path(article), method: :delete, data: { confirm: "Are you sure?"}   %>
</p>
<hr>
</div>
<% end %>


